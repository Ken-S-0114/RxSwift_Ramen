  target 'RxSwift_RamenTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'RxBlocking', '~> 4.0'
    pod 'RxTest',     '~> 4.0'
  end

  target 'RxSwift_RamenUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
